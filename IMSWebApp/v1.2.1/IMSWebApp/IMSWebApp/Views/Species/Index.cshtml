@model IEnumerable<IMSWebApp.Models.Species>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.min.js"></script>

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Common)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Scientific)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Common)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Scientific)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPk">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPk">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPk">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
